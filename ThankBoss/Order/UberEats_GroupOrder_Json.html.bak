<!DOCTYPE html>
<html lang="zh-Hant">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>JSON Parser</title>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
$(document).ready(function(){
    $("#parseBtn").click(function(){
		var selectedText = window.getSelection().toString();
		console.log(selectedText);
        var jsonDataStr = $("#jsonData").val();
        try {
            var jsonData = JSON.parse(jsonDataStr); // 將字串轉換成 JavaScript 物件
			var feedCards_length = jsonData.data.orders[0].feedCards.length
			feedCards_length -= 1
            if (jsonData.data && jsonData.data.orders && jsonData.data.orders[0] && jsonData.data.orders[0].feedCards && jsonData.data.orders[0].feedCards[feedCards_length] && jsonData.data.orders[0].feedCards[feedCards_length].orderSummary && jsonData.data.orders[0].feedCards[feedCards_length].orderSummary.sections) {
                var parsedData = parseFeedCards(jsonData.data.orders[0].feedCards[feedCards_length].orderSummary.sections);
				$("#StoreName").text(jsonData.data.orders[0].orderInfo.storeInfo.name);
				$("#activeOrderStatus").text(jsonData.data.orders[0].activeOrderStatus.subtitleSummary.summary.text);
                displayTable(parsedData); // 輸出表格
				console.log($("#StoreName").text());
            } else {
                console.error("JSON 格式不符合預期，缺少必要的屬性。");
            }
        } catch (error) {
            console.error("解析 JSON 時發生錯誤：", error);
        }
    });
    
    function parseFeedCards(feedData) {
        // 創建一個空的陣列來存儲解析後的資料
        var parsedData = [];
        
        // 迭代 feedData 中的每一個部分
        $.each(feedData, function(index, section) {
            // 提取訂購人的名稱
            var buyer = section.header.title;
            
            // 迭代該部分中的每一個訂購項目
            $.each(section.items, function(i, item) {
                // 提取訂購品項的名稱
                var itemName = item.title;
                
                // 提取訂購數量
                var quantity = item.quantity;
                
                // 提取總金額
                var totalPrice = item.price;
                
                // 將提取的資料組合成一個物件並添加到 parsedData 陣列中
                parsedData.push({
                    buyer: buyer,
                    itemName: itemName,
                    quantity: quantity,
                    totalPrice: totalPrice,
                    discount: 0 // 初始化優惠額為 0
                });
            });
        });
        
        // 返回解析後的資料
        return parsedData;
    }
    
    function displayTable(data) {
		// 清除之前的表格
		$("#result").empty();
		
		// 創建表格標題行
		var table = $("<table>").addClass("table");
		var thead = $("<thead>").appendTo(table);
		var trHead = $("<tr>").appendTo(thead);
		$("<th>").text($("#StoreName").text()).appendTo(trHead);
		$("<th>").text("訂購品項").appendTo(trHead);
		$("<th>").text("訂購數量").appendTo(trHead);
		$("<th>").text("原金額").appendTo(trHead);
		$("<th>").text("優惠額").appendTo(trHead); // 新增優惠額欄位
		$("<th>").text("應付額").appendTo(trHead); // 新增應付額欄位
		$("<th>").text("已付").appendTo(trHead); // 新增已付欄位
		
		// 添加資料到表格中
		var tbody = $("<tbody>").appendTo(table);
		$.each(data, function(index, item) {
			var tr = $("<tr>").appendTo(tbody);
			$("<td>").text(item.buyer).appendTo(tr);
			$("<td>").text(item.itemName).appendTo(tr);
			$("<td>").text(item.quantity).appendTo(tr);
			$("<td>").text(item.totalPrice).appendTo(tr);
			var discountInput = $("<input>").attr("type", "number").attr("step", "0.01").addClass("discount").appendTo($("<td>").appendTo(tr)); // 優惠額輸入框
			$("<td>").text(item.totalPrice).appendTo(tr); // 初始應付額為原金額
			$("<td>").append($("<input>").attr("type", "checkbox")).appendTo(tr); // 新增已付欄位
		});
		
		// 將表格加入到結果區域中
		$("#result").append(table);
		
		// 監聽優惠額輸入框的變化事件
		$(document).on("input", ".discount", function() {
			// 取得該行的所有 <td> 元素
			var rowTds = $(this).closest("tr").find("td");
			
			// 取得原金額
			//var originalPrice = parseFloat(rowTds.eq(3).text());
			var originalPrice = parseFloat(rowTds.eq(3).text().replace("$", "").trim());

			// 取得優惠額
			var discount = parseFloat($(this).val());
			
			// 計算應付額
			var payableAmount = originalPrice - discount;
			// 更新應付額欄位的顯示值，保留兩位小數
			rowTds.eq(5).text(payableAmount.toFixed(2));
		});
	}

	function Export() {
		var csvContent = "";	//"data:text/csv;charset=utf-8,";
	
		// 添加CSV標題行
		var headers = [$("#StoreName").text(), "原價","減免額度","減免金額計算", "減免金額", "應付金額","已收","收款人",$("#payee").val()];
		csvContent += headers.join(",") + "\n";
		
		var TotalPrice = 0,TotalDiscount = 0
		// 獲取表格中的資料行
		$("#result table tbody tr").each(function () {
			var row = $(this);
			var discountInput = row.find("td:eq(4) input");
			var discountValue = discountInput.val() !== "" ? discountInput.val() : "0"; // 若優惠額為空，則填0
			var rowData = [
				row.find("td:eq(0)").text(), // 訂購人
				row.find("td:eq(3)").text().replace("$", "").trim(), // 原金額
				"",
				"",
				discountValue.replace("$", "").trim(), // 優惠額
				parseInt(row.find("td:eq(5)").text().replace("$", "").trim()) * parseInt($("#off").val()) / 100, // 應付額
				row.find("td:eq(6) input[type=checkbox]").is(":checked") ? "V" : ""
			];
			console.log(parseInt($("#off").val()));
			csvContent += rowData.join(",") + "\n";
			TotalPrice += parseInt(row.find("td:eq(3)").text().replace("$", "").trim());
			TotalDiscount += parseInt(discountValue.replace("$", "").trim());
		});
		csvContent += "總原價" + "," + TotalPrice + ",,,總應付," + parseInt((parseInt(TotalPrice) - parseInt(TotalDiscount)) * parseInt($("#off").val())) / 100 + ",,,,折扣金額," + parseInt(TotalDiscount);
		
		// 將CSV數據匯出到console
		console.log(csvContent);
		
		var link = window.document.createElement("a");
		link.setAttribute("href", "data:text/csv;charset=utf-8,%EF%BB%BF" + encodeURI(csvContent));
		link.setAttribute("download", "input.csv");
		link.click();
	};

    // 匯出 CSV 按鈕點擊事件
    $("#export").click(function(){
        // 呼叫匯出函數
        Export();
    });
});
</script>
